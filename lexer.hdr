enum TokenKind
{
    BAD_TOKEN,
    EOI,
    DECIMAL_LITERAL,
    IDENTIFIER,
    PLUS,
    LPAREN,
    RPAREN,
    ASTERISK,
};

type TokenVal: array[12] of char;

struct Token
{
    kind:   TokenKind;
    pos:    struct Pos {
                row, col: unsigned;
            };
    val:    TokenVal;
};

extern token: Token;
extern fn getToken(): Token;
extern fn tokenKindStr(: TokenKind): -> char;
